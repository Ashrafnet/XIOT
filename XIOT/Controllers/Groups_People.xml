  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Groups_People"."GroupID" "GroupID"
	,"Group"."GroupCode" "GroupGroupCode"
	,"Groups_People"."PeopleID" "PeopleID"
	,"People"."Name" "PeopleName"
	,"Groups_People"."DateCreated" "DateCreated"
	,"Groups_People"."DateModified" "DateModified"
from "dbo"."Groups_People" "Groups_People"
	left join "dbo"."LU_Groups" "Group" on "Groups_People"."GroupID" = "Group"."GroupID"
	left join "dbo"."DIM_People" "People" on "Groups_People"."PeopleID" = "People"."PeopleID"
]]></text>
